            <field name="name">Redes Sociales</field>
            <field name="res_model">posik_client.social_network</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="social_network_menu_root" name="Redes Sociales" sequence="30"  action="action_social_network"/> 
    


        
